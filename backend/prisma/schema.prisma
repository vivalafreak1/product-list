// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Product {
  id          String @id 
  name        String
  imageUrl    String
  stock       Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

